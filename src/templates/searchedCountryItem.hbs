{{#each this}}
<h1 class="countryName"> {{name}}</h1>

<ul class="countryInfoList">
    <li><span class="text"><b>Capital:</b> </span>{{capital}}</li>
    <li><span class="text"><b>Population:</b> </span>{{population}}</li>
   
    <li class="langList">
        <span class="text"><b>Languages:</b></span>
        <ul >
        {{#each languages}}
            <li>{{name}}</li>
        {{/each}}
        </ul>
    </li>
</ul>

<div class="img-thumb">    
<img src="{{flag}}" alt="" class="flag" width="600" />
</div>

{{/each}}